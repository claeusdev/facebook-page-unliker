{{#each likes as |page| }}
    <div class="like-cell pv3 ph3 flex items-center justify-between">
      <div>
        <h4 class="mt0"><a class="blue underline-hover"
               target="_blank"
               href="{{page.link}}">{{page.name}}</a></h4>
        <h5>Liked on {{moment-format page.created_time 'LL'}}</h5>
        <span>{{page.fan_count}} people like this page.</span>
      </div>

      <div>
        <div {{action "clickedUnlike" page}} class="fb-like"
          data-href={{page.link}} data-layout="button" data-action="like"
          data-show-faces="false" data-share="false">Button is loading...</div>
      </div>
    </div>
{{/each}}
